/// @notice Generate beast sprite component using base64 encoded PNG
/// @dev Creates beast sprite for battle interface using optimized PNG image
/// @param x X position for the sprite
/// @param y Y position for the sprite
/// @return SVG content for beast sprite with embedded PNG
pub fn generate_beast_sprite(x: u32, y: u32) -> ByteArray {
    let mut sprite = "";

    // Embedded base64 PNG data for beast sprite
    let beast_png_base64 =
        "";

    // Create SVG image element with embedded PNG
    sprite += "<image x=\"";
    sprite += format!("{}", x);
    sprite += "\" y=\"";
    sprite += format!("{}", y);
    sprite += "\" width=\"103\" height=\"107\" href=\"data:image/png;base64,";
    sprite += beast_png_base64;
    sprite += "\"/>";

    sprite
}

